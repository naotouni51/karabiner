{
  "title": "enthumble(Windows App) Mode",
  "rule_list": false,
  "make_extra_description": true,
  "rules": [
    {
      "description": "(enthumble) IJKL Mode / normal",
      "extra_descriptions": [
        "無変換/英数 + J, K, I, L -> カーソル移動（←, ↓, ↑, →）",
        "無変換/英数 単独 -> 英数キー"
      ],
      "extra_description_level": 1,
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "international5",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "fn",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_eisuu"
            }
          ]
        },
        {
